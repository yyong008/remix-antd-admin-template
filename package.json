{
  "name": "remix-antd-admin-template",
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "keywords": [
    "remix",
    "admin",
    "office websit",
    "echarts-liquidfill-ssr",
    "ssr",
    "prettier",
    "antd",
    "pro-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yyong008/remix-antd-admin.git"
  },
  "author": "Magnesium",
  "license": "MIT",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "test": "vitest",
    "commit": "git-cz",
    "changeset": "changeset",
    "version": "changeset version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.42",
    "@remix-run/css-bundle": "^2.3.0",
    "@remix-run/node": "^2.3.0",
    "@remix-run/react": "^2.3.0",
    "@remix-run/serve": "^2.3.0",
    "antd": "^5.11.2",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@remix-run/dev": "^2.3.0",
    "@remix-run/eslint-config": "^2.3.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
   "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
